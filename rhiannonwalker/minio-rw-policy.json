{
"Version": "2012-10-17",
"Statement": [
    {
    "Action": [
        "s3:GetBucketLocation",
        "s3:ListBucket"
    ],
    "Effect": "Allow",
    "Resource": [
        "arn:aws:s3:::rhiannonwalker",
        "arn:aws:s3:::rhiannonwalker-dev",
        "arn:aws:s3:::k8s-pages/rhiannonwalker",
        "arn:aws:s3:::k8s-pages/rhiannonwalker-dev"
    ]
    },
    {
    "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:ListMultipartUploadParts",
        "s3:AbortMultipartUpload"
    ],
    "Effect": "Allow",
    "Resource": [
        "arn:aws:s3:::rhiannonwalker/*",
        "arn:aws:s3:::rhiannonwalker-dev/*",
        "arn:aws:s3:::k8s-pages/rhiannonwalker/*",
        "arn:aws:s3:::k8s-pages/rhiannonwalker-dev/*"
    ]
    }
]
}
